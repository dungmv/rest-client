{
  "identifier": "migrated",
  "description": "permissions that were migrated from v1",
  "local": true,
  "windows": [
    "main"
  ],
  "permissions": [
    "sql:allow-load",
    "sql:allow-execute",
    "path:default",
    "event:default",
    "window:default",
    "app:default",
    "resources:default",
    "menu:default",
    "tray:default",
    "window:allow-center",
    "window:allow-maximize",
    "window:allow-minimize",
    "window:allow-close",
    "window:allow-start-dragging",
    "shell:allow-open",
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "https://*/"
        },
        {
          "url": "http://*/"
        }
      ]
    },
    "shell:default",
    "http:default"
  ]
}